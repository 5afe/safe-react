    "run-with-testrpc": "0.3.1",
    "style-loader": "1.1.4",
    "terser-webpack-plugin": "2.3.5",
    "truffle": "5.1.21",
    "truffle-contract": "4.0.31",
    "truffle-solidity-loader": "0.1.32",
    "url-loader": "4.1.0",
    "webpack": "4.42.1",
    "webpack-bundle-analyzer": "3.7.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0"
  }
}
