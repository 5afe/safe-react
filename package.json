    "@babel/preset-env": "7.9.5",
    "@babel/preset-flow": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "@testing-library/react": "10.0.2",
    "autoprefixer": "9.7.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.3.0",
    "babel-loader": "8.1.0",
